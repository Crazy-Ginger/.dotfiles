! ##############
! URxvt config##
! ##############
! special
*.foreground:   #bebebe
*.background:   #000000
*.cursorColor:  #bebebe
URxvt*depth:        32


! black
*.color0:       #000000
*.color8:       #404040

! red
*.color1:       #f23838
*.color9:       #ffa1a1

! green
*.color2:       #77bb33
*.color10:      #dcf692

! yellow
*.color3:       #efc334
*.color11:      #f6df92

! blue
*.color4:       #5555dd
*.color12:      #b2baf6

! magenta
*.color5:       #bb38ff
*.color13:      #f09fff

! cyan
*.color6:       #3af2ed
*.color14:      #64d3d0

! white
*.color7:       #9e9e9e
*.color15:      #eeeeee


URxvt.font: xft:Hack:pixelsize=16:antialias=true
URxvt.boldFont: xft:Hack-Bold:pixelsize=16:antialias=true
URxvt.italicFont: xft:Hack-Italic:pixelsize=16:antialias=true
URxvt.letterSpace: -3
URxvt.lineSpace: 1

URxvt.transparent: true
URxvt.shading: 30

URxvt.scrollBar: false

!look into copying and pasting with shortcuts
!URxvt.keysym.Shift-Control-V: eval:paste_clipboard
!URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
